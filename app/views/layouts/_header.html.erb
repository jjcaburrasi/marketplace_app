<% if worker_signed_in? %>
    <%= render 'header_partials/worker_header' %>
<% elsif client_signed_in? %>
    <%= render 'header_partials/client_header' %>
<% elsif admin_signed_in? %>
    <%= render 'header_partials/admin_header' %>
<% else %>
    <%= render 'header_partials/visitor_header' %>
<%end%>
  